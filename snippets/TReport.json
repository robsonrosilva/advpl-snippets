            "    oSection5b := oReport:Section(1):Section(1):Section(1):Section(1):Section(1)",
            "",
            "    oSection1b:BeginQuery()",
            "    BeginSQL Alias ccAlias",
            "        %noparser%",
            "",
            "        ${query}",
            "",
            "    EndSql",
            "    oSection1b:EndQuery()",
            "",
            "    oSection2b:SetParentQuery()",
            "    oSection3b:SetParentQuery()",
            "    oSection4b:SetParentQuery()",
            "    oSection5b:SetParentQuery()",
            "",
            "    oReport:SetMeter((ccAlias)->(RecCount()))",
            "",
            "    oSection2b:SetParentFilter({|cParam| (ccAlias)->${cellSession1} == cParam}, {|| (ccAlias)->${cellSession1}})",
            "",
            "    oSection3b:SetParentFilter({|cParam| (ccAlias)->(${cellSession1} + ${cellSession2})  == cParam}, {|| (ccAlias)->(${cellSession1} + ${cellSession2})})",
            "",
            "    oSection4b:SetParentFilter({|cParam| (ccAlias)->(${cellSession1} + ${cellSession2} + ${cellSession3})  == cParam}, {|| (ccAlias)->(${cellSession1} + ${cellSession2} + ${cellSession3})})",
            "",
            "    oSection5b:SetParentFilter({|cParam| (ccAlias)->(${cellSession1} + ${cellSession2} + ${cellSession3} + ${cellSession4})  == cParam}, {|| (ccAlias)->(${cellSession1} + ${cellSession2} + ${cellSession3} + ${cellSession4})})",
            "",
            "    oSection1b:Print()",
            "return",
            "",
